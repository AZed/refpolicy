## <summary>System log analyzer and reporter</summary>

########################################
## <summary>
##	Read logwatch temporary files.
## </summary>
## <param name="domain">
##	Domain allowed access.
## </param>
#
interface(`logwatch_read_tmp_files',`
	gen_require(`
		type logwatch_tmp_t;
	')

	files_search_tmp($1)
	allow $1 logwatch_tmp_t:file r_file_perms;
')
